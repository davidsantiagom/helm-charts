{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "GetTenantSecrets",
      "Effect": "Allow",
      "Action": [
        "secretsmanager:GetSecretValue",
        "secretsmanager:DescribeSecret"
      ],
      "Resource": "arn:aws:secretsmanager:eu-central-2:058264250936:secret:t001-dev/*",
      "Condition": {
        "StringEquals": {
          "aws:ResourceTag/tenant-id": "t001-dev"
        }
      }
    }
  ]
}
